lumn":14}}],"sources":{"scss/_reset.scss":"/* http://meyerweb.com/eric/tools/css/reset/ \n   v2.0 | 20110126\n   License: none (public domain)\n*/\n\nhtml, body, div, span, applet, object, iframe,\nh1, h2, h3, h4, h5, h6, p, blockquote, pre,\na, abbr, acronym, address, big, cite, code,\ndel, dfn, em, img, ins, kbd, q, s, samp,\nsmall, strike, strong, sub, sup, tt, var,\nb, u, i, center,\ndl, dt, dd, ol, ul, li,\nfieldset, form, label, legend,\ntable, caption, tbody, tfoot, thead, tr, th, td,\narticle, aside, canvas, details, embed, \nfigure, figcaption, footer, header, hgroup, \nmenu, nav, output, ruby, section, summary,\ntime, mark, audio, video {\n\tmargin: 0;\n\tpadding: 0;\n\tborder: 0;\n\tfont-size: 100%;\n\tfont: inherit;\n\tvertical-align: baseline;\n}\n/* HTML5 display-role reset for older browsers */\narticle, aside, details, figcaption, figure, \nfooter, header, hgroup, menu, nav, section {\n\tdisplay: block;\n}\nbody {\n\tline-height: 1;\n}\nol, ul {\n\tlist-style: none;\n}\nblockquote, q {\n\tquotes: none;\n}\nblockquote:before, blockquote:after,\nq:before, q:after {\n\tcontent: '';\n\tcontent: none;\n}\ntable {\n\tborder-collapse: collapse;\n\tborder-spacing: 0;\n}","scss/_home.scss":".home {\n  .info-section {\n    width: 35%;\n    position: absolute;\n    top: 0;\n    bottom: 0;\n    left: 0;\n    padding: 55px;\n\n    .logo {\n      border: 4px solid #fff;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      font-size: 2rem;\n      color: white;\n      line-height: 0;\n      height: 150px;\n      width: 200px;\n    }\n    .info-box {\n      bottom: 60px;\n      position: absolute;\n      h4 {\n        font-size: 1.2rem;\n        color: $color3;\n        margin: 0.7rem 0;\n      }\n      p {\n        font-size: 0.9rem;\n        font-weight: 300;\n        color: $color3;\n        margin: 0.2rem 0 1.5rem;\n      }\n    }\n    .icon-section {\n      a {\n        color: white;\n        text-decoration: none;\n        font-size: 2rem;\n        padding: 0 10px;\n        &:first-child {\n          padding: 0 10px 0 0;\n        }\n        &:hover {\n            color: $hoverColor;\n\n        }\n      }\n    }\n  }\n  .content-area {\n    margin-left: 35%;\n    background: $color3;\n    width: 65%;\n    height: 100%;\n    // padding: 0 0 0 25%;\n    .img-container {\n      width: 30%;\n      display: inline-block;\n      position: relative;\n      height: 100%;\n\n      .user-img {\n        position: absolute;\n        top: 56px;\n        left: -250px;\n        width: 500px;\n        height: calc(100% - 112px);\n        background: url(\"../img/jeffrey.jpg\") no-repeat center center;\n        -webkit-background-size: cover;\n        -moz-background-size: cover;\n        -o-background-size: cover;\n        background-size: cover;\n        -webkit-box-shadow: 0px 0px 74px 17px rgba(0,0,0,0.34);\n        -moz-box-shadow: 0px 0px 74px 17px rgba(0,0,0,0.34);\n        box-shadow: 0px 0px 74px 17px rgba(0,0,0,0.34);\n      }\n    }\n    .profile-section {\n      vertical-align: top;\n      display: inline-block;\n      // width: 70%;\n      max-width: 500px;\n      padding: 20% 0 0;\n\n      .profile {\n        h1 {\n          font-size: 2.3rem;\n          font-weight: 500;\n          line-height: 1.2;\n          margin: 0 0 1rem;\n          span {\n            color: $color1;\n          }\n        }\n\n        h5 {\n          font-size: 1.1rem;\n          margin: 0 0 2rem;\n        }\n        p {\n          color: $color5;\n          font-weight: 300;\n          line-height: 1.4;\n          margin: 0 0 2rem;\n        }\n      }\n    }\n  }\n}\n","scss/_variable.scss":"$background: red;\n$color1: #4b21ff;\n$color2: #272727;\n$color3: #fefefe;\n$color4: #272727;\n$color5: #a9a9a9;\n$color6: #bdbdbd;\n$hoverColor: #f8ff21\n\n\n","scss/main.scss":"@import \"reset.scss\";\n@import \"variable\";\n@import \"home\";\n\n* {\n  box-sizing: border-box;\n}\n\nhtml,\nbody {\n  background: $color1;\n  font-size: 18px;\n  font-family: \"Roboto Mono\", monospace;\n  height: 100%;\n}\n\n.menu-icon {\n  position: fixed;\n  right: 36px;\n  top: 36px;\n  padding: 20px;\n  z-index: 11;\n  cursor: pointer;\n  .fa-solid {\n    font-size: 2rem;\n    color: $color1;\n  }\n}\n\n.primary-btn {\n  background: $color1;\n  -webkit-border-radius: 30px;\n  -moz-border-radius: 30px;\n  border-radius: 30px;\n  text-align: center;\n  color: $color3;\n  padding: 15px 30px;\n  display: inline-block;\n  text-transform: uppercase;\n  letter-spacing: 3px;\n  margin-right: 15px;\n  text-decoration: none;\n  transition: all .3s ease-in-out;\n  &:hover {\n    background: black;\n    color: $color3;\n\n  }\n}\n\n.ghost-btn {\n    text-decoration: none;\n  border: 4px solid $color1;\n  -webkit-border-radius: 30px;\n  -moz-border-radius: 30px;\n  border-radius: 30px;\n  text-align: center;\n  color: $color1;\n  padding: 12px 27px;\n  display: inline-block;\n  text-transform: uppercase;\n  letter-spacing: 3px;\n  transition: all .3s ease-in-out;\n  &:hover {\n    border-color: $hoverColor;\n  }\n}\n\n\n  #menu {\n    background: $color1;\n    position: fixed;\n    width: 100%;\n    top: 0;\n    bottom: 0;\n    transform: translate3d(-100%, 0,0);\n    z-index: 10;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    opacity: 1;\n    transition: all 0.5s ease-in-out;\n\n    nav {\n      width: 400px;\n      text-align: center;\n      a {\n        display: block;\n        font-size: 2.3rem;\n        padding: 1rem;\n        text-decoration: none;\n        color: white;\n        &:hover {\n          color: $color5;\n        }\n      }\n    }\n  }\n\n\n.menu-active {\n  #menu {\n    width: 100%;\n    height: 100%;\n    top: 0;\n    bottom: 0;\n    transform: translate3d(200px, 0,0);\n  }\n\n  .menu-icon {\n    .fa {\n        color: white\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"a35316c8bfc1f00e8ceee9c6cbc24880","cacheData":{"env":{}}}